{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "restParams": true
  },
  "rules": {
    "func-names": 0,
    "comma-dangle": [2, "never"],
    "space-before-function-paren": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "react/jsx-no-bind": 0,
    "prefer-arrow-callback": 0,
    "max-len": 0,
    "camelcase": 0
  }
}
